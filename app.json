{
  "expo": {
    "name": "트립스케치",
    "slug": "tripsketch",
    "version": "1.0.1",
    "updates": {
      "enabled": true,
      "url": "https://u.expo.dev/29ddc7fe-a117-470b-8787-7c52553fa0fb"
    },
    "runtimeVersion": "1.0.1",
    "orientation": "portrait",
    "icon": "./src/assets/logoIconBackground.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/splashScreen.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*", "assets/images/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "kr.kro.tripsketch"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/logoIconBackground.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": ["android.permission.RECORD_AUDIO"],
      "package": "kr.kro.tripsketch"
    },
    "web": {
      "favicon": "./src/assets/logoIconBackground.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "notification": {
      "handler": "BackgroundNotificationHandler",
      "experienceId": "@shagrat/tripsketch"
    },
    "extra": {
      "eas": {
        "projectId": "29ddc7fe-a117-470b-8787-7c52553fa0fb"
      }
    }
  }
}
